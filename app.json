{
	"expo": {
		"name": "UVic",
		"slug": "UVic",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/img/Martlet-blue.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/img/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/img/Martlet-blue.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/img/Martlet-blue.png"
		},
		"plugins": ["expo-secure-store"]
	}
}
